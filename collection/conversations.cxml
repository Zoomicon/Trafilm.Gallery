        <Item Id="LoveActually.Lake Scene" Name="LoveActually.Lake Scene" Img="#1" Href="http://gallery.trafilm.net/conversation/metadata/?film=LoveActually&amp;conversation=LoveActually.Lake Scene">
            <Description>Available in YouTube: https://www.youtube.com/watch?v=wmmojewOIv4</Description>
            <Facets>
                <Facet Name="Film Reference Id">
                    <String Value="LoveActually" />
                </Facet>
                <Facet Name="Info created">
                    <DateTime Value="2016-06-01T04:23:13" />
