        <Facet Name="Reference Id">
          <String Value="FellowshipOfTheRing.Introduction_Galadriel.Off_intro.SpanishDub" />
        </Facet>
        <Facet Name="Transcription">
          <String Value="I amar prestar aen.&#xD;&#xA;&#xD;&#xA;El mundo ha cambiado.&#xD;&#xA;&#xD;&#xA;Han mathon ne nen.&#xD;&#xA;&#xD;&#xA;Lo siento en el agua.&#xD;&#xA;&#xD;&#xA;Han mathon ne chae.&#xD;&#xA;&#xD;&#xA;Lo siento en la tierra.&#xD;&#xA;&#xD;&#xA;A han noston ned gwilith.&#xD;&#xA;&#xD;&#xA;Lo huelo en el aire." />
        </Facet>
